<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!DOCTYPE REPORT SYSTEM "PHPReport.dtd">
<REPORT MARGINWIDTH="5" MARGINHEIGHT="5">

	<BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>
	<CSS>reports/css/reportS1.css</CSS>	
	<SQL>SELECT account_name C1, account_number C2, B.user_name C3, C.source_name C4, D.industry_name C5, employees C6, revenue C7, A.created C8, E.user_name C9, A.updated C10, F.user_name C11, A.note C12 FROM `accounts` A, users B, sources C, industries D, users E, users F WHERE A.user_id = B.user_id AND A.source_id = C.source_id AND A.industry_id = D.industry_id AND A.created_by = E.user_id AND A.updated_by = F.user_id ORDER BY account_name ASC</SQL>
	<INTERFACE>mysql</INTERFACE>
	<CONNECTION>localhost</CONNECTION>
	<DATABASE>demo</DATABASE>
	<NO_DATA_MSG>No data was found, check your query</NO_DATA_MSG>
	
	<TITLE>Accounts</TITLE>
   	<BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>
	<DOCUMENT>
		<HEADER>
			<ROW>
				<COL COLSPAN="1" >
					<XHTML>
						<a><IMG SRC="api/images/print.gif" onclick="javascript:this.src='api/images/invisible.gif';print();"/></a>
					</XHTML>
				</COL>
			</ROW>
		</HEADER>
	</DOCUMENT>
	<PAGE CELLSPACING="0" CELLPADDING="0" SIZE="5000">
		<HEADER>
			<ROW>
				<COL ALIGN="center" TYPE="EXPRESSION" CELLCLASS="HEADER1UL" TEXTCLASS="BOLD" colspan="6" >$this->getParameter("range_date")</COL>
			</ROW>
			<ROW>
				<COL CELLCLASS="HEADERFIELDS">Account</COL>
				<COL CELLCLASS="HEADERFIELDS">N</COL>
				<COL CELLCLASS="HEADERFIELDS" >Owner</COL>
				<COL CELLCLASS="HEADERFIELDS" >Source</COL>
				<COL CELLCLASS="HEADERFIELDS" >Industry</COL>
				<COL CELLCLASS="HEADERFIELDS" >Employees</COL>
				<COL CELLCLASS="HEADERFIELDS" ALIGN="RIGHT" >Revenue</COL>
				<COL CELLCLASS="HEADERFIELDS" >Created</COL>
				<COL CELLCLASS="HEADERFIELDS" >Created.by</COL>
				<COL CELLCLASS="HEADERFIELDS" >Updated</COL>
				<COL CELLCLASS="HEADERFIELDS" >Updated.by</COL>
				<COL CELLCLASS="HEADERFIELDS" >Note</COL>
			</ROW>
		</HEADER>	
		<FOOTER>
   		</FOOTER>
	</PAGE>
	<GROUPS>
		<GROUP NAME="QUARTER" EXPRESSION="QU">
			<FIELDS>
				<ROW>
					<COL TYPE="FIELD" CELLCLASS="HEADERFIELDS" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD">C1</COL>
					<COL TYPE="FIELD" CELLCLASS="HEADERFIELDS" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD">C2</COL>
					<COL TYPE="FIELD" CELLCLASS="HEADERFIELDS" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD">C3</COL>
					<COL TYPE="FIELD" CELLCLASS="HEADERFIELDS" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >C4</COL>
					<COL TYPE="FIELD" CELLCLASS="HEADERFIELDS" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >C5</COL>
					<COL TYPE="FIELD" CELLCLASS="HEADERFIELDS" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >C6</COL>
					<COL TYPE="FIELD" CELLCLASS="HEADERFIELDS" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" NUMBERFORMATEX="2" ALIGN="RIGHT" >C7</COL>
					<COL TYPE="FIELD" CELLCLASS="HEADERFIELDS" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >C8</COL>
					<COL TYPE="FIELD" CELLCLASS="HEADERFIELDS" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >C9</COL>
					<COL TYPE="FIELD" CELLCLASS="HEADERFIELDS" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >C10</COL>
					<COL TYPE="FIELD" CELLCLASS="HEADERFIELDS" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >C11</COL>
					<COL TYPE="FIELD" CELLCLASS="HEADERFIELDS" CELLCLASSEVEN="EVEN" CELLCLASSODD="ODD" >C12</COL>
				</ROW>
			</FIELDS>
		</GROUP>
	</GROUPS>
</REPORT>
