	<SQL>
		SELECT A.account_name C1, A.main_account C2, A.status_name C3, A.first_names C4, A.last_name C5, A.title C6, A.phone C7, 
			A.employees C8, A.address C9, A.city C10, A.zipcode C11, A.state_id C12, B.user_name C13
